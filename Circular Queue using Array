#include <stdio.h>
#define MAX 5
int q[MAX],f=-1,r=-1;
void insert(){
    int x; if((r+1)%MAX==f) printf("Full\n");
    else{printf("Enter: ");scanf("%d",&x);
        if(f==-1)f=0; r=(r+1)%MAX; q[r]=x;}
}
void del(){
    if(f==-1)printf("Empty\n");
    else{printf("Del: %d\n",q[f]); if(f==r)f=r=-1; else f=(f+1)%MAX;}
}
void disp(){
    if(f==-1)printf("Empty\n");
    else{int i=f;while(1){printf("%d ",q[i]);
        if(i==r)break;i=(i+1)%MAX;}printf("\n");}
}
int main(){
    int c;while(1){
        printf("1.In 2.Del 3.Disp 4.Quit ");scanf("%d",&c);
        switch(c){case 1:insert();break;case 2:del();break;case 3:disp();break;case 4:return 0;}
    }
}
